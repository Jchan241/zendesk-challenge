<div class="ticket-wrapper">
  <div class="container my-3">
    <h1>tickets</h1>

    <% @tickets.each do |ticket| %>
      <div class="card">
        <p><%= ticket.subject %></p>
        <%= link_to ticket_path(ticket.id), class: "card-link" do %>
        <% end %>
      </div>
    <% end %>

    <%= will_paginate @tickets %>
  </div>
</div>
